#include<stdio.h>
main()
{
 int H,a,b;
 printf("enter a two number");
scanf ("%d %d",&a,&b);
for(H=a<b?a:b; H<=1;H--)
if(a%H==0 &&b%H==0)
break;
printf("HCFof two number is=%d",H);
 
}

